#include <stdio.h> 
 int main() { 
     int n; 
     printf("Enter the seat number : "); 
     scanf("%d", &n); 
     if(n<=10){ 
         printf("Lower double"); 
     }else if(n<=15){ 
         printf("Lower single"); 
     }else if(n<=25){ 
         printf("Upper double"); 
     }else if(n<=30){ 
         printf("Upper single"); 
     }else{ 
         printf("Invalid seat number"); 
     }  
     return 0; 
 }
