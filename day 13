#include <stdio.h>
int main()  {
      int T;
      scanf("%d",&T);
      while (T - -)  {
             int A , B , X , Y;
             scanf("%d %d %d %d", &A, &B, &X, &Y);
             int total_power = X * Y;
             if (total_power >= A * B) {
                 printf("Yes\n");
             }  else  {
                 printf("No\n");
             }
      }
      return 0;
}
