#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int X, Y;
        scanf("%d %d", &X, &Y);

        // Calculate the minimum number of extra chairs needed
        int extra_chairs = (X > Y) ? (X - Y) : 0;

        printf("%d\n", extra_chairs);
    }

    return 0;
}
